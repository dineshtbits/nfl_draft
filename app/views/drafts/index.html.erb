<p id="notice"><%= notice %></p>

<h1>All Drafts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3">Select Draft</th>
    </tr>
  </thead>
  <tbody>
    <% @drafts.each do |draft| %>
      <tr>
        <td><%= link_to "#{draft.name}-#{draft.year}", draft %></td>
      </tr>
    <% end %>
  </tbody>
</table>